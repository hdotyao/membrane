T
{
    internalField   uniform 300;

    boundaryField
    {
        wall
        {
            type            fixedValue;
            value           uniform 300;
        }

        "shell_to_.*"
        {
            type            fixedValue;
            value           uniform 300;
        }

        "inlet|outlet"
        {
            type            zeroGradient;
        }
    }
}

U
{
    internalField   uniform (0 0 0);

    boundaryField
    {
        inlet
        {
            type            fixedValue;
            value           uniform ( 0 0 -0.0005 );
        }
        
        outlet
        {
            type            zeroGradient;
        }
        
        wall
        {
            type            fixedValue;
            value           uniform ( 0 0 0 );
        }

        "shell_to_.*"
        {
            type            fixedValue;
            value           uniform ( 0 0 0 );
        }
    }
}

